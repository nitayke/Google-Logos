{
    "manifest_version": 2,
    "name" : "Logos ",
    "version" : "1.1" ,
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"],
            "run_at" : "document_end"
        }
    ],
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
    "permissions": ["tabs"] 
}
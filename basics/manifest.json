{
    "manifest_version": 2,
    "name" : "google logos",
    "version" : "1.1" ,
    "content_scripts" :[
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"],
            "run_at" : "document_end"
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["tabs"] 
}